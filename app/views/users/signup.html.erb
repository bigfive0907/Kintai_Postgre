<div class="main user-new">
  <div class="container">
    <div class="form-heading">新規ユーザー登録</div>
    <div class="form user-form">
      <div class="form-body">
        
      
        <%= form_tag("/user/create") do %>
          <p>ユーザー名</p>
          <input name="name" value="">
          <p>メールアドレス</p>
          <input name="email" value="">
          <p>パスワード</p>
          <input type="password" name="password" value="">
          <input type="submit" value="新規登録">
        <% end %>
      </div>
    </div>
  </div>
</div>